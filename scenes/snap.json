{
    "name": "snap",
    "description": "Laser show with disco ball and strobe effects",
    "key_command": "s",
    "midi": 83,
    "lights": [
        {
            "name": "lasers",
            "type": "dimmer",
            "modulator": "mfft",
            "mfft_range": [0, 20],
            "power_range": [0.5, 3],
            "brightness_range": [0, 255]
        },
        {
            "name": "laser",
            "type": "laser",
            "modulator": "mfft",
            "mfft_range": [0, 20],
            "power_range": [0.3, 0.8],
            "zoom_range": [0, 127],
            "speed_range": [0, 255]
        },
        {
            "name": "pinspot",
            "type": "pinspot",
            "modulator": "mfft",
            "mfft_range": [0, 20],
            "power_range": [0.5, 3],
            "brightness_range": [0, 255],
            "color": "blue",
            "strobe": 240
        },
        {
            "name": "strobe",
            "type": "strobe",
            "modulator": "mfft",
            "threshold": 0.2,
            "brightness": 255,
            "mfft_range": [115, 127],
            "power_range": [0.6, 1.0],
            "speed": 1
        }
    ]
}